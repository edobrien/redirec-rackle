{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-snappy": "^0.4.3",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.4",
        "google/recaptcha": "^1.2",
        "fx3costa/laravelchartjs": "^2.2",
        "gregwar/image": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/http": "6.*",
        "illuminate/support": "6.*",
        "jeroendesloovere/vcard": "^1.7",
        "laravel/framework": "6.20.16",
        "laravel/helpers": "^1.4",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "5.1.3",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "6.*",
        "league/oauth1-client": "^1.0",
        "maatwebsite/excel": "3.1.26",
        "puz/dynamic-mail": "^0.1.1",
        "rvanlaak/wkhtmltopdf-amd64-centos7": "0.12.2.1",
        "spatie/icalendar-generator": "^2.1",
        "shuchkin/simplexlsx": "^0.8.9",
        "yajra/laravel-datatables-oracle": "9.*"
    },
    "require-dev": {
        "illuminate/contracts": "6.*",
		"fakerphp/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/Helper.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
